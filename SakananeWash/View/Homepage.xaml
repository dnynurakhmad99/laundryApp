            <Button x:Name="btnProfile" Content="Button" Height="30" Canvas.Left="82" Canvas.Top="594" Width="30" Opacity="0" Click="btnProfile_Click"/>
            <Button x:Name="btnProgress" Content="Button" Canvas.Left="24" Canvas.Top="594" Width="30" Height="30" Opacity="0" Click="btnProgress_Click"/>
            <Button x:Name="btnCuci" Content="Button" Canvas.Left="91" Canvas.Top="476" Width="143" Opacity="0" Height="37" Click="btnCuci_Click"/>
            <Button x:Name="btnHistory" Content="Button" Canvas.Left="266" Canvas.Top="594" Width="30" Height="30" Opacity="0" Click="btnHistory_Click"/>
            <Button x:Name="btnChat" Content="Button" Canvas.Left="209" Canvas.Top="594" Width="30" Opacity="0" Height="30" Click="btnChat_Click"/>
        </Canvas>

    </StackPanel>
</Page>
